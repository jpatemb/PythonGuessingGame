n = random.randint(1, 10)
count = 1
guess = int(input("I'm thinking of a number between 1 an 10. Can you guess what number it is?"))
while n != "guess":
    print
    if guess < n:
        print ("Too low")
        count += 1
        guess = int(input("Guess again:"))
    elif guess > n:
        print("Too high")
        count += 1
        guess = int(input("Guess again:"))
    elif guess == n:
        print("Correct! You guessed the number in " + str(count) + " tries.")
        break
    else:
        print("Invalid input!")
